{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "properties": {
    "apiVersion": {
        "type": "string",
        "enum": [ "v1alpha3" ]
      },
      "metadata": {
            "type": "object",
            "properties": {
            "name": {
                "type" : "string"
            },
            "config": {
                "type" : "string"
            },
            "email": {
                "type" : "string"
            }
          },
          "required": ["name","config", "email"]
    }
  },
  "required": ["apiVersion","metadata"]
}